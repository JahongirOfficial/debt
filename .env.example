# Environment variables for Qarzdaftar application

# Port for the backend server (default is 5002)
VITE_BACKEND_PORT=5002

# MongoDB connection URI (default is mongodb://localhost:27017/qarzdaftar)
MONGODB_URI=mongodb://localhost:27017/qarzdaftar

# JWT secret key (default is qarzdaftar_jwt_secret_key)
JWT_SECRET=qarzdaftar_jwt_secret_key